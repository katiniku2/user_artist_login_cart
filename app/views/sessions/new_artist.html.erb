<h1>Artist log in</h1>

<div class="row">
	<div class="col-md-6 col-md-offset-3">
		<%= form_for(:session, url: login_artist_path) do |f| %>

			<%= f.label :email %>
			<%= f.email_field :email, class: 'form-contrl' %>

			<%= f.label :password %>
			<%= f.password_field :password, class: 'form_control' %>

			<%= f.submit "Artist log in", class: "btn btn-primary" %>
		<% end %>

		<P>New Artist? <%= link_to "Sign up now!", signup_artist_path %></P>
	</div>
</div>